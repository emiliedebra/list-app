{
  "name": "list-app",
  "version": "1.0.0",
  "description": "Shopping List Backend",
  "main": "index.mjs",
  "repository": "https://github.com/emiliedebra/list-app.git",
  "author": "Emilie Wood <emilie@anotherway.co.za>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@awt-tooling/eslint-config-awt-flow": "^1.1.1",
    "@std/esm": "^0.20.0",
    "eslint": "^4.17.0",
    "flow-remove-types": "^1.2.3",
    "nodemon": "^1.14.12"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "flow-bin": "^0.65.0",
    "jsonwebtoken": "^8.1.1",
    "mocha": "^5.0.0",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.3"
  },
  "scripts": {
    "test": "yarn run build && mocha -r flow-remove-types/register -r @std/esm './test/*.mjs'",
    "dev": "yarn run build && nodemon --experimental-modules ./lib/index.mjs",
    "flow": "flow",
    "flow:check": "flow check ./src",
    "build": "rm -rf ./lib && flow-remove-types -p -d ./lib/ ./**/*.mjs && flow-remove-types -p -d ./lib/ *.mjs"
  },
  "@std": {
    "esm": {
      "esm": "all",
      "cjs": true
    }
  }
}
